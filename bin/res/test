fun makeCounter() {
    var i = 0;
    var life = fun (){
        print "life is good";
    };
    // var ff = fun (){
    // };
    return life;
}
var lifeIsGood = makeCounter();
lifeIsGood();

var i = 1;
while (i < 20) {
    shw(i+"\n");
    if(i > 5) {i = i+2; continue;}
    if (i == 10)break;
    i = i + 1;
}
for(var i = 0; i < 20; i = i + 1){
    shw(i+"\n");
    if(i > 5) {i = i+2; continue;}
    if (i == 10)break;
}

class DevonshireCream {
    serveOn() {
        return "Scones";
    }
}
print DevonshireCream; 

class Bagel {}
var bagel = Bagel();
print bagel;

var a = 3 ? "a" : "b"; 
print a; 

fun thrice(fn) {
    for (var i = 1; i <= 3; i = i + 1) {
        fn(i);
    }
}
thrice(fun (a){ print a;});
    // "1".
    // "2".
    // "3".
class Bacon {
    eat() {
        print "Crunch crunch crunch!";
    }
}
Bacon().eat();
class Doughnut {
    cook() {
        print "Fry until golden brown.";
    }
}
class BostonCream < Doughnut {}
BostonCream().cook();


var fuck = fun (a){ 
    print "fuck "+a;
};
fuck("sex");
shw(type(fuck));

const f1 = fun (x) {
    return x * x;
};
shw(f1(2));

const f2 = fun (a, b){
    return a * b;
};
shw(f2(5, 5));

var factor = 10;
const f3 = fun (x){ return x * factor; };
shw(f3(5));

var f4 = fun(){ return fun(y){ return y + 1; }; };
shw(f4()(2));


var a = "global";
{
fun showA() {
print a;
}
showA();
var a = "block";
showA();
}

var a = "before";
var f = fun () {
    print a;
};
f();
a = "after";
f();
